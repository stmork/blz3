# Beispiel Makefile

CPPFLAGS	= @CPPFLAGS@
LIBS		= libB3System_ind.o

LIB_SOURCE =\
	b3CPUBase.cc\
	b3Date.cc\
	b3Display.cc\
	b3Exception.cc\
	b3LogBase.cc\
	b3PluginBase.cc\
	b3Row.cc\
	b3TimeAbstract.cc

LIB_OBJ	=\
	b3CPUBase.o\
	b3Date.o\
	b3Display.o\
	b3Exception.o\
	b3LogBase.o\
	b3PluginBase.o\
	b3Row.o\
	b3TimeAbstract.o

all:	$(LIB_OBJ)

depend:
	gcc -M $(LIB_SOURCE) $(CPPFLAGS) >.dep

clean:
	rm -f *.o *.a *.bak *~ core $(PRGS) $(LIBS) ALL QUIET

-include .dep
